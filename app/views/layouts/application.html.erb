<!DOCTYPE html>
<html>
<head>
  <title>RorProject</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/css/materialize.min.css" %>

  <%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/js/materialize.min.js" %>
  <%= stylesheet_link_tag "http://fonts.googleapis.com/icon?family=Material+Icons" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:errors] %>
<% flash[:errors].each do |error| %>
<%= error %>
<% end %> 
<% end %>

<% if flash[:msg] %>
<%= flash[:msg] %>
<% end %> 

<%= yield %>

</body>
</html>
